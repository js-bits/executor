{
  "name": "@js-bits/executor",
  "version": "2.0.9",
  "description": "Abstract Executor class",
  "keywords": [
    "javascript",
    "typescript",
    "abstract",
    "executor",
    "pattern",
    "receiver",
    "promise",
    "timing",
    "latency",
    "measurement"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "default": "./index.js"
    }
  },
  "scripts": {
    "build": "rimraf ./dist && yarn build:dts && rollup ./index.js --format cjs --file dist/index.cjs --exports named",
    "build:dts": "tsc ./index.js --allowJs --emitDeclarationOnly --declaration --esModuleInterop --outDir dist && yarn replace:symbols ./dist/src/executor.d.ts",
    "replace:symbols": "node node_modules/@js-bits/enumerate/scripts/replace-unique-symbols",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --verbose",
    "test-cjs": "jest --verbose",
    "test-watch": "yarn test --watch",
    "lint": "tsc --noEmit && eslint '**/*.{js,jsx,ts,tsx}'",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/js-bits/executor"
  },
  "author": "https://github.com/apolo-gh",
  "license": "ISC",
  "dependencies": {
    "@js-bits/enumerate": "^1.0.17",
    "@js-bits/performance": "^1.0.2",
    "@js-bits/timeout": "^1.0.10",
    "@js-bits/xpromise": "^1.0.12"
  },
  "devDependencies": {
    "@js-bits/formalinter": "^1.0.3",
    "@js-bits/log-in-color": "^1.0.2",
    "@types/jest": "29.4.3",
    "husky": "^8.0.1",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "rimraf": "^5.0.1",
    "rollup": "^3.23.1",
    "typescript": "^4.8.4"
  },
  "engines": {
    "node": ">=12"
  },
  "prettier": "@js-bits/formalinter/.prettierrc.json",
  "eslintConfig": {
    "extends": "@js-bits/eslint-config/.eslintrc.json"
  },
  "jest": {
    "projects": [
      {
        "displayName": "node",
        "testEnvironment": "node"
      },
      {
        "displayName": "jsdom",
        "testEnvironment": "jsdom"
      }
    ]
  }
}
